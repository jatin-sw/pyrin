[general]

# default maximum number of items that could be stored
# in a cache before the need to remove old items.
# this value may not be used in some handlers.
default_max_length: 10000

# default timeout for cached items invalidation in milliseconds.
default_timeout: 20

# default caching handler to be used.
default_caching_handler: simple

[simple]

# maximum number of items that could be stored
# in a cache before the need to remove old items.
max_length: 10000

# timeout for cached items invalidation in milliseconds.
timeout: 20

[memcached]

# timeout for cached items invalidation in milliseconds.
timeout: 20
